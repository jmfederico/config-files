# Source original activate file.
source venv/bin/activate

# Make sure python know we can handle UTF-8
# Plus, I like spanish!
export LC_ALL=es_ES.UTF-8
export LANG=es_ES.UTF-8

# My local settings file is to be loaded.
export ENVIRONMENT='local'

# Set my project's environmental variables.
export A_REQUIRED_VARIABLE='Something to keep private'
export ANOTHER_REQUIRED_VAR='Something that changes a lot.'

export SECRET_KEY='a-v3ry-random-str1ng'
export DEBUG='1'

export BASE_HOST='localhost'
# export AN_OPTIONAL_VARIABLE='which is not set, because it is optional'
